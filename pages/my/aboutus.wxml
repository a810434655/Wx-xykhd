<!--pages/my/aboutus.wxml-->
<view class="container b-background">
  <i-row i-class='b-white'>
    <i-col span="24" i-class='centerv vertical-center b-white' style="display:flex;margin-bottom:20rpx">
      <image mode='aspectFit' style='width:100%;height:200rpx;display:block;margin:40rpx 0px 0rpx 0rpx' src="/resources/image/ic_logo.png">
      </image>
      <view class='c-title-30 center' style='display:block;padding-top:20rpx'>校园酷活动</view>
      <view class='c-content-info-24 center' style='display:block;padding-top:20rpx;margin-bottom:20rpx'>V3.0.0</view>
      <view class='center b-background' style='mix-height:1px;width:100%'>
      </view>
    </i-col>
    <i-col span="24" i-class="b-white">
      <i-col span="22" i-class='cell fix' bindtap='openPage' data-url='/pages/my/feedback'>
        <view class='c-title-30' style='line-height:70rpx;display:inline-block;margin:0rpx 20rpx'>意见反馈</view>
        <view class='cell-ft'></view>
      </i-col>
      <view class='b-background' style='min-height:1rpx;width:100%;float:left' />
      <i-col span="22" i-class='cell fix' bindtap='openH5Page' data-url='https://h5.dreamoncampus.com/file/html/privacypolicy.html'>
        <view class='c-title-30' style='line-height:70rpx;display:inline-block;margin:0rpx 20rpx'>隐私保护协议</view>
        <view class='cell-ft'></view>
      </i-col>
      <view class='b-background' style='min-height:1rpx;width:100%;float:left' />
      <i-col span="22" i-class='cell fix' bindtap='openH5Page' data-url='https://h5.dreamoncampus.com/file/html/agreement.html'>
        <view class='c-title-30' style='line-height:70rpx;display:inline-block;margin:0rpx 20rpx'>平台自律公约</view>
        <view class='cell-ft'></view>
      </i-col>
    </i-col>
  </i-row>
  <text class='c-content-info-24 center' style='min-width:100%;display:block;padding-top:0rpx;position:absolute;bottom:250rpx'>贵州句芒聚能科技有限公司出品</text>
  <official-account></official-account>
</view>