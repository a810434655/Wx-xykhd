/* pages/ui/lottery.wxss */

.prize-col {
  position: relative;
}

.active {
  width: 128rpx;
  height: 136rpx;
  z-index: 1;
  -webkit-transition: all 0.04s ease-in;
  -moz-transition: all 0.04s ease-in;
  transition: all 0.04s ease-in;
}

.b-red{
  background-image:url(data:image/png;base64,);background-repeat:no-repeat; background-size:100% 100%;
  color: #FFF;
  font-weight: 700;
  font-size: 30rpx;
  padding: 20rpx 0;
  margin: 0rpx 10rpx 20rpx 10rpx;
}
.b-blue{
  background-image:url(data:image/png;base64,);background-repeat:no-repeat; background-size:100% 100%;
  color: #FFF;
  font-weight: 700;
  font-size: 30rpx;
  padding: 20rpx 0;
  margin: 0rpx 10rpx 20rpx 10rpx;
}

.bx{
  font-size: 22rpx;
}

.ll{
  font-size: 24rpx;
  font-weight: 700;
  color: #7c1e01;
  margin-bottom: 20rpx
}

.hid {
  opacity: 0;
}

.prize {
  width: 104rpx;
  height: 108rpx;
  position: absolute;
  top: 11rpx;
  left: 15rpx;
  z-index: 10;
}
.prize-button{
  margin-top: 10rpx;
  width: 228rpx;
  height: 178rpx;
}
.prize-surplus{
  font-family: SourceHanSansCN-Bold;
	font-size: 28rpx;
	font-weight: bold;
	font-stretch: bold;
	line-height: 37rpx;
	letter-spacing: 0px;
	color: #fff3d0;
}
.modal-mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 1000;
  transition: all 0.2s ease-in-out;
  opacity: 0;
  visibility: hidden;
}
.modal-mask.show{
  opacity:1;
  visibility:visible;
}
.model-button{
  margin: 20rpx 20rpx 0rpx 20rpx;
}
.notice{
  width:570rpx;
  height: 68rpx;
  background-color: #000000;
  border-radius: 34px;
  background: rgba(0,0,0,0.3);
  position: absolute;
  z-index:500;
  top: 34rpx;
  -webkit-animation:notice 15s ease 0s;
  -o-animation:notice 15s ease 0s;
  animation:notice 15s ease 0s;
  overflow:hidden;
}
.notice-content{
  width: 470rpx;
  margin-left: 50rpx;
}
.text{
  text-align: center;
  color: #ffffff;
  opacity: 1;
  position:relative;
  z-index:10;
  width: 470rpx;
	font-family: SourceHanSansCN-Bold;
	font-size: 27rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 50rpx;
	letter-spacing: 0px;
}
@keyframes notice{
  0%{
     background: rgba(0,0,0,0);
   }
   100%{
     background: rgba(0,0,0,0.3);
   }
}
